package formatTask;

public class FormatTask01 {
	public static void main(String[] args) {
//		식을 완성시키시오 (format으로 출력하기)
//		10 + 20 = 30
//		7 * 8 = 56
//		6 * 4 = 24
		
		int numOfTen = 10;	
		int numOfTwenty = 20;
		int numOfSeven = 7;
		int numOfEight = 8;
		int numOfSix = 6;
		int numOfFour = 4;
		
		System.out.printf("%d+%d =" ,numOfTen+numOfTwenty, numOfTen, numOfTwenty );
	}
}
