package variableTask;

public class VariableTask02 {
	public static void main(String[] args) {
//      5, 7845.15, false, B, 85.10F, 자바, S, 98590, D, true, 공백문자
//      저장공간 2개만 사용해서 모든 값을 출력하기
//      datas, numbers
		int num_1 = 0;
		float num_2 = 0.0F;
		boolean data_1 = false;
		char data_2 = ' ';
		float num_3 = 0.0F;
		String data_3 = null;
		char data_4 = ' ';
		int num_4 = 0;
		char data_5 = ' ';
		boolean data_6 = false;
		String data_7 = null;
		float numbers = 0.0F;
		String datas = null;
		
		num_1 = 5;
		num_2 = 7845.15F;
		data_1 = false;
		data_2 = 'B';
		num_3 = 85.10F;
		data_3 = "자바";
		data_4 = 'S';
		num_4 = 98590;
		data_5 = 'D';
		data_6 = true;
		data_7 = "";
			
		numbers = num_1;
		
	}
}
