package castingTask;

public class CastingTask02 {
   public static void main(String[] args) {
//      "8.43" + 2.59를 더해서 10이 나오도록 출력하기
	   String num_1 = "8.43";
	   double num_2 = 2.59;
	   
	   double number_1 = Double.parseDouble(num_1);
	   int result_1 = (int)number_1, result_2 = (int)num_2;
			  
	   System.out.println(result_1 + result_2);
	   
   }
}
