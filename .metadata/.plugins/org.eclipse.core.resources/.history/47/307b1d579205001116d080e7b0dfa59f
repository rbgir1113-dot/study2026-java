package castingTask;

public class CastingTask02 {
   public static void main(String[] args) {
//      "8.43" + 2.59를 더해서 10이 나오도록 출력하기
	   String num_1 = "8.43";
//	   double num_2 = 2.59;
	   
	   int number_1 = (int)Integer.parseInt(num_1);
	   System.out.println(number_1);
			  
	   
	   
   }
}
